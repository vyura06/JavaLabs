<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<StackPane focusTraversable="true" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/8.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
    <children>
        <ImageView fx:id="menuImg" fitHeight="600.0" fitWidth="450.0" pickOnBounds="true" smooth="false" />
        <Pane fx:id="gamePane" disable="true" focusTraversable="true" visible="false" />
        <StackPane>
            <children>
                <VBox fx:id="menuPane" alignment="CENTER" disable="true" spacing="20.0" visible="false">
                    <children>
                        <Button fx:id="b1" mnemonicParsing="false" onAction="#tempInvoke" prefWidth="120.0" text="НОВАЯ ИГРА" />
                        <Button fx:id="b2" mnemonicParsing="false" onAction="#settings" prefWidth="120.0" text="НАСТРОЙКИ" />
                        <Button fx:id="b10" mnemonicParsing="false" onAction="#infoAction" prefWidth="120.0" text="ОБ ИГРЕ" />
                        <Button fx:id="b3" mnemonicParsing="false" onAction="#exitMenu" prefWidth="120.0" text="ВЫХОД" />
                    </children>
                </VBox>
                <VBox fx:id="tempPane" alignment="CENTER" disable="true" spacing="20.0" visible="false">
                    <children>
                        <Button fx:id="b4" mnemonicParsing="false" onAction="#newGame" prefWidth="120.0" text="ЗАПУСТИТЬ" />
                        <Button fx:id="b5" mnemonicParsing="false" onAction="#exitTemp" prefWidth="120.0" text="НАЗАД" />
                    </children>
                </VBox>
                <VBox fx:id="settingsPane" alignment="CENTER" spacing="20.0" visible="false">
                    <children>
                        <Button fx:id="b6" mnemonicParsing="false" onAction="#control" prefWidth="120.0" text="УПРАВЛЕНИЕ" />
                        <Button fx:id="b12" mnemonicParsing="false" onAction="#soundSettings" prefWidth="120.0" text="ЗВУК" />
                        <Button fx:id="b7" mnemonicParsing="false" onAction="#settingsExit" prefWidth="120.0" text="НАЗАД" />
                    </children>
                </VBox>
                <VBox fx:id="pausePane" alignment="CENTER" spacing="20.0" visible="false">
                    <children>
                        <ImageView fx:id="pauseIMG" fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" />
                        <Button fx:id="b8" mnemonicParsing="false" onAction="#contin" prefWidth="120.0" text="ПРОДОЛЖИТЬ" />
                        <Button fx:id="b9" mnemonicParsing="false" onAction="#pauseExit" prefWidth="120.0" text="В МЕНЮ" />
                    </children>
                </VBox>
                <VBox fx:id="info" alignment="CENTER" spacing="20.0" visible="false">
                    <children>
                        <ImageView fx:id="infoIMG" fitHeight="496.0" fitWidth="384.0" pickOnBounds="true" preserveRatio="true" />
                        <Button fx:id="b11" mnemonicParsing="false" onAction="#infoExit" prefWidth="120.0" text="В МЕНЮ" />
                    </children>
                </VBox>
                <VBox fx:id="soundPane" alignment="CENTER" spacing="20.0" visible="false">
                    <children>
                        <ImageView fx:id="toggle" fitHeight="150.0" fitWidth="200.0" onMouseClicked="#mouseCliked" pickOnBounds="true" preserveRatio="true" />
                        <Button fx:id="b14" mnemonicParsing="false" onAction="#soundExit" prefWidth="120.0" text="НАЗАД" />
                    </children>
                </VBox>
                <VBox fx:id="controlPane" alignment="CENTER" spacing="20.0" visible="false">
                    <children>
                        <ImageView fx:id="controlIMG" fitHeight="496.0" fitWidth="384.0" pickOnBounds="true" preserveRatio="true" />
                        <Button fx:id="b15" mnemonicParsing="false" onAction="#controlExit" prefWidth="120.0" text="НАЗАД" />
                    </children>
                </VBox>
            </children>
        </StackPane>
    </children>
</StackPane>
